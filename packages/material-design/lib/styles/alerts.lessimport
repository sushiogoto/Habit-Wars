.alert {
    border: 0px;
    border-radius: 0;
    a, .alert-link {
        color: @darkbg-text;
    }
    .variations(~"", background-color, @darkbg-text);
    &-info, &-danger, &-warning, &-success {
        color: @darkbg-text;
    }
    &-default {
        a, .alert-link {
            color: @lightbg-text;
        }
    }
}
